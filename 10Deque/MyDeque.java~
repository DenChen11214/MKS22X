public class MyDeque{
    private int start;
    private int end;
    private String[] data;
    public MyDeque(){
	data = new String[10];
    }
    public myDeque(int initialCapacity){
	if(initialCapacity < 0){
	    throw new IllegalArgument();
	}
	data = new String[initialCapacity];
    }
    public int size(){
	return data.length;
    }
    public void addFirst(String value){
	if(value == null){
	    throw new NullPointerException();
	}
    }
}
